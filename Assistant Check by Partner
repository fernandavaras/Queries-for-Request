SELECT AL1.PART_NUMBER, AL1.PART_DESCRIPTION, AL3.CUST_ID, AL3.PART_ORDERED, AL3.QSTR, AL3.NEAREST_RETURN_DATE, AL4.SUPPLIER_CBN, AL4.CUST_ID, AL4.Company_Name,

TRUNC(AL3.nearest_return_date) as returnDate

FROM WIZADMIN.PART_INFO AL1, WIZADMIN.LINK_PART_SUPPLIER AL2, WIZADMIN.VI_DEFECTIVE AL3, WIZADMIN.COMPANY_PROFILE AL4 

WHERE (AL1.PART_NUMBER=AL2.PART_NUMBER AND AL4.CUST_ID=AL3.CUST_ID AND AL2.PART_NUMBER=AL3.PART_ORDERED) 
AND AL2.supplier_id = 1 AND AL3.qstr > 0 AND AL3.defective_return_flag = 'Y' AND AL3.cust_id = 609 AND AL4.Cust_ID = AL3.Cust_ID

ORDER BY AL1.part_number

//// change Cust.id as it need it ////
