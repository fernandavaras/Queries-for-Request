SELECT 
AL1.PART_NUMBER, 
AL1.PRODUCT_NUMBER, 
AL2.PRODUCT_DESCRIPTION, 
AL2.PRODUCT_LINE_ID, 
AL2.INTRO_DATE, 
AL2.WTY_CODE, 
AL2.PRODUCT_TYPE, 
AL2.PRODUCT_FAMILY, 
AL2.PRODUCT_CYCLE, 
AL2.FACTORY, 
AL2.SKU_ADDED 
FROM WIZADMIN.LINK_PART_PRODUCT AL1, WIZADMIN.PRODUCT_INFO AL2 WHERE (AL2.PRODUCT_NUMBER=AL1.PRODUCT_NUMBER)  AND (AL2.INTRO_DATE>={ts '2014-01-01 00:00:00.000'})

